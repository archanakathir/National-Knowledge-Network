<h1>Listing national_long_distances</h1>

<table>
  <thead>
    <tr>
      <th>Nld name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @national_long_distances.each do |national_long_distance| %>
      <tr>
        <td><%= national_long_distance.nld_name %></td>
        <td><%= link_to 'Show', national_long_distance %></td>
        <td><%= link_to 'Edit', edit_national_long_distance_path(national_long_distance) %></td>
        <td><%= link_to 'Destroy', national_long_distance, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New National long distance', new_national_long_distance_path %>
