<h1>Listing isps</h1>

<table>
  <thead>
    <tr>
      <th>Isp</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @isps.each do |isp| %>
      <tr>
        <td><%= isp.isp %></td>
        <td><%= link_to 'Show', isp %></td>
        <td><%= link_to 'Edit', edit_isp_path(isp) %></td>
        <td><%= link_to 'Destroy', isp, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Isp', new_isp_path %>
